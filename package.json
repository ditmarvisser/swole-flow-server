{
  "name": "swole-flow-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "nodemon src/server.ts",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc -p .",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo",
    "migrate:rollback:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:regenerate": "npm run migrate:rollback:all & npm run migrate & npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ditmarvisser/swole-flow-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ditmarvisser/swole-flow-server/issues"
  },
  "homepage": "https://github.com/ditmarvisser/swole-flow-server#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "nodemon": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
